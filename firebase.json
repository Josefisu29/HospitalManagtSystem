{
  "hosting": {
    "public": ".",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "/admin",
        "destination": "/admin.html"
      },
      {
        "source": "/doctor",
        "destination": "/doctor/doctor.html"
      },
      {
        "source": "/doctor/*",
        "destination": "/doctor/*.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },

   [{
      "source": "/admin/**",
      "destination": "/admin/admin.html"
    }
    {
      "source": "**",
      "destination": "/index.html"
    }
  
}]

  "database": {
    "rules": "database.rules.json"
  }

